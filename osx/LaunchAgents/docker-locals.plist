<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>KeepAlive</key>
        <true/>
        <key>Label</key>
        <string>docker-composer local services</string>
        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/bin/docker-compose</string>
            <string>start</string>
            <string>postgres</string>
            <string>devpi</string>
        </array>

        <key>RunAtLoad</key>
        <true/>

        <key>WorkingDirectory</key>
        <string>/data/PROGETTI/saxix/dotfiles/docker</string>

        <key>StandardErrorPath</key>
        <string>/usr/local/var/log/docker-services.error.log</string>

        <key>StandardOutputPath</key>
        <string>/usr/local/var/log/docker-services.log</string>

    </dict>
</plist>
